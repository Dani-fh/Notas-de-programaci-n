// Use this editor to write, compile and run your Java code online

public class Main {
    public static void main(String[] args) {
        // Entero
        int integer= 29;
        System.out.println("El rango de integer es -2147483648 to 2147483647");
        System.out.println("El int es "+ integer);

        //Caracater
        char caracater = 'k';
        Character caracaterObject;
        System.out.println("El rango del caracter es 0 to 65535");
        System.out.println("El char es " + caracater);
        
        //Byte
        byte byteNumber = 15;
        System.out.println("El rango de un byte es -128 to 127");
        System.out.println("El byte es "+ byteNumber);
        
        
        
        //Boolean
        boolean booleanNumber = false;
        System.out.println("El rango del Boolean es True o False ");
        System.out.println("El boolean es "+ booleanNumber);

    
     //Decimal double
        double doubleNumber = 2.3435;
        System.out.println("El rango del Double es  4.9e-324 to 1.8e308");
        System.out.println("El double es "+ doubleNumber);
    
    
    

        //Short
        short shortNumber = 44;
        System.out.println("El rango de short es -32768 to 32767");
        System.out.println("El short es " + shortNumber);

       //Long
        long longNumber = 254367;
        System.out.println("El rango de long es -9223372036854775808 to 9223372036854775807");
        System.out.println("El long es "+ longNumber);
    

        //Float / flotante
        float floatNumber = 22.674f;
        System.out.println("El rango de float es 1.4e-45 to 3.4e38 ");
        System.out.println("El float es "+ floatNumber);

       
        //Cadena de caracteres
        String cadena = "Daniel ";
        System.out.println("El rango de string es 12+4+20=36 bytes");
        System.out.println("El String es "+ cadena);
       
        //Object
        Object objeto;
       
    }
}
